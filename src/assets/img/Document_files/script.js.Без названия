function hello() {
    console.log('Hello!', this);
}

const person = {
    name: 'Vladilen',
    age: 19,
    sayHello: hello,
    sayHelloWindow: hello.bind(document),
    logInfo: function() {
        console.log(`Name is ${this.name}`)
        console.log(`Age is ${this.age}`);
    },
}

const lena = {
    name: 'Elena',
    age: 20,
}